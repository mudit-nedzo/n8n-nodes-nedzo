{
  "name": "n8n-nodes-nedzo",
  "version": "1.0.12",
  "description": "n8n node for Nedzo AI voice agent automation platform",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "nedzo",
    "voice",
    "ai",
    "automation",
    "phone",
    "calling"
  ],
  "license": "MIT",
  "homepage": "https://nedzo.ai",
  "author": {
    "name": "Nedzo",
    "email": "support@nedzo.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mudit-nedzo/n8n-nodes-nedzo.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "echo 'Linting skipped for initial release'",
    "lintfix": "echo 'Linting skipped for initial release'",
    "test": "echo 'No tests defined yet' && exit 0",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/NedzoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Nedzo/Nedzo.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.120.3",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
